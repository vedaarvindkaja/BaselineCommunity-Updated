    <validationRules>
        <fullName>Approver_Cooment</fullName>
        <active>true</active>
        <description>If Approver Reject a timesheet, It Approver Notes shouldn&apos;t empty.</description>
        <errorConditionFormula>AND( 
ISPICKVAL(TR1__Status__c,&quot;Rejected&quot;), ISBLANK(TR1__Approvers_Notes__c) 
)</errorConditionFormula>
        <errorMessage>Approver Notes can&apos;t emtpy when Rejecting the Timeseet</errorMessage>
    </validationRules>

