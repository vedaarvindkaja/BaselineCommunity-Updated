    <validationRules>
        <fullName>RC_Post_Externally_True</fullName>
        <active>true</active>
        <errorConditionFormula>AND( TR1__Post_Externally__c == true, ISBLANK( TR1__External_Job_Title__c ) )</errorConditionFormula>
        <errorMessage>External Job Title can&apos;t be empty if job can be posted externally</errorMessage>
    </validationRules>

     <fields>
        <fullName>Job_Title__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISBLANK(TR1__External_Job_Title__c), TR1__Job_Title__c , TR1__External_Job_Title__c )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Job Title</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
